{
 "manifest_version": 2,
 "name": "My New Tab",
 "description": "Track your tasks whenever you open a new tab",
 "version": "1.1",
 "author": "Rohan Yadav",
"browser_action": {
   "default_icon": "images/tab_icon.png",
   "default_title": "Have a good day"
 },
"chrome_url_overrides" : {
  "newtab": "newtab.html"
},
 "permissions": [
   "storage"
 ]
}
